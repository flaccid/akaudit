import sys
import os

if os.environ.get('LC_CTYPE', '') == 'UTF-8':
	os.environ['LC_CTYPE'] = 'en_US.UTF-8'
import akaudit.clidriver

def main():
    return akaudit.clidriver.main()

if __name__ == '__main__':
    sys.exit(main())
